{
    "manifest_version": 3,
    "name": "Chrome Copy Paste Extension",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "https://developer.chrome.com/*"
    ],
    "permissions": [
        "tabGroups",
        "activeTab",
        "clipboardWrite",
        "clipboardRead"
    ]
}